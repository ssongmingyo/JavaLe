package chap04_controlstatement;

public class _01_IfStatement {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num = 12345;
		
		// num가 짝수인지 홀수인지 출력하는 조건문을 작성하세요.
		if(num % 2 == 0) {
			System.out.println("짝수");
		} else {
			System.out.println("홀수");
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	
